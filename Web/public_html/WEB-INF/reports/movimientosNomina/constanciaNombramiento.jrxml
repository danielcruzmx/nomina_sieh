<?xml version="1.0" encoding="UTF-8"?>
<jasperReport xmlns="http://jasperreports.sourceforge.net/jasperreports" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://jasperreports.sourceforge.net/jasperreports http://jasperreports.sourceforge.net/xsd/jasperreport.xsd" name="ConstanciaNombramiento" pageWidth="612" pageHeight="792" columnWidth="555" leftMargin="20" rightMargin="20" topMargin="20" bottomMargin="20" isIgnorePagination="true">
	<property name="ireport.zoom" value="1.5"/>
	<property name="ireport.x" value="0"/>
	<property name="ireport.y" value="344"/>
	<parameter name="IMAGES_URI" class="java.lang.String"/>
	<parameter name="PAR_ID_MOVTO" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[SELECT
    CASE WHEN TN_MOVTOS_EMP_PLA.ID_GRUPO_PAGO = 'V' THEN 'Eventual' WHEN TN_MOVTOS_EMP_PLA.ID_NOMBRAMIENTO_NOM = 'B' THEN 'Base' WHEN TN_MOVTOS_EMP_PLA.ID_NOMBRAMIENTO_NOM = 'C' THEN 'Confianza' END NOMBRAMIENTO,
    CASE WHEN TC_ATRIBUTO_PUESTO.ID_JERARQUIA <= '6' OR TN_MOVTOS_EMP_PLA.ID_PUESTO_NOM = 'HONORAR' THEN 'Mixto'  WHEN TC_ATRIBUTO_PUESTO.ID_JERARQUIA  > '6' THEN 'Diurno' END HORARIO,
    TC_MOVS_PERSONAL.DESC_MOVS_PERSONAL,
    SUBSTR(TD_EMPLEADO.RFC_UNICO,1,4)||'   '||SUBSTR(TD_EMPLEADO.RFC_UNICO,5,6)||'   '||SUBSTR(TD_EMPLEADO.RFC_UNICO,11,13) RFC_UNICO,
    TD_EMPLEADO.PRIMER_APELLIDO,
    TD_EMPLEADO.SEGUNDO_APELLIDO,
    TD_EMPLEADO.NOMBRE_EMPLEADO,
    TD_EMPLEADO.CURP_EMPLEADO,
    TC_GENERO.DESC_GENERO,
    CASE WHEN TD_EMPLEADO.ID_GENERO = 'F' AND TD_EMPLEADO.ID_EDO_CIVIL = 'C' THEN 'Casada' WHEN TD_EMPLEADO.ID_GENERO = 'M' AND TD_EMPLEADO.ID_EDO_CIVIL = 'C' THEN 'Casado' WHEN TD_EMPLEADO.ID_GENERO = 'F' AND TD_EMPLEADO.ID_EDO_CIVIL <> 'C' THEN 'Soltera' WHEN TD_EMPLEADO.ID_GENERO = 'M' AND TD_EMPLEADO.ID_EDO_CIVIL <> 'C' THEN 'Soltero' END ESTADO_CIVIL,
    TC_NACIONALIDAD.DESC_NACIONALIDAD,
    CASE WHEN SUBSTR(TD_EMPLEADO.CURP_EMPLEADO, 11,11) = 'M' THEN 'DOMICILIO    PARTICULAR    DE    LA    TRABAJADORA' WHEN SUBSTR(TD_EMPLEADO.CURP_EMPLEADO, 11,11) = 'H' THEN 'DOMICILIO    PARTICULAR    DEL    TRABAJADOR' ELSE 'DOMICILIO    PARTICULAR    DEL    TRABAJADOR' END TITULO_DOMICILIO,
    CASE WHEN TN_MOVTOS_EMP_PLA.ID_MOVTO_PERSONA = '602' THEN '' /*TC_REFERENCIA.DOMICILIO_OFICIAL || '        COLONIA: ' || TC_REFERENCIA.COLONIA_OFICIAL || '      C.P. ' || TC_REFERENCIA.CODPOS_OFICIAL || '      DELEGACIÓN O MUNICIPIO: ' ||    MUNI_OFI.DESCRIPCION || '        ENTIDAD FEDERATIVA: '  || EDO_OFI.DESC_ESTADO || '      TELÉFONO:  ' || TC_REFERENCIA.TELEFONO_OFICIAL*/
           ELSE TD_EMPLEADO.CALLE_PARTICULAR || '    Colonia: ' || TD_EMPLEADO.COLONIA_PARTICULAR || '  C.P. ' || TD_EMPLEADO.CODPOST_PARTICULAR || '  ' || 'Delegación o Municipio: ' || TC_MUNICIPIO.DESC_MUNICIPIO || '    Entidad Federativa: '  || TC_ESTADO.DESC_ESTADO || '  Teléfono:  '|| TD_EMPLEADO.TEL_PARTICULAR END DOMICILIO,
    LPAD(TN_MOVTOS_EMP_PLA.ID_PLAZA,6,'000000') ID_PLAZA,
    TN_MOVTOS_EMP_PLA.ID_UNIDAD_NOM,
    TC_UNIDAD.DESC_UNIDAD,
    CASE WHEN TN_MOVTOS_EMP_PLA.MEP_QNA_CAPTURA >= 201616 and tn_movtos_emp_pla.id_unidad_nom <> 'G00' THEN
            case when TC_ATRIBUTO_PUESTO.id_jerarquia <= '5' and TN_MOVTOS_EMP_PLA.id_unidad_nom <> 'G00' and TN_MOVTOS_EMP_PLA.id_puesto_nom not in (' P05377',' P05395',' P05380') --diferente de G00 y Laudos
                         then substr(TN_MOVTOS_EMP_PLA.id_puesto_nom,1,4)||substr(TN_MOVTOS_EMP_PLA.id_puesto_nom,7,1) ||'00'||substr(TN_MOVTOS_EMP_PLA.id_puesto_nom,5,3)
                    when TC_ATRIBUTO_PUESTO.id_jerarquia <= '5' and TN_MOVTOS_EMP_PLA.id_unidad_nom <> 'G00' and TN_MOVTOS_EMP_PLA.id_puesto_nom in (' P05377',' P05395',' P05380') --Laudos
                         then substr(TN_MOVTOS_EMP_PLA.id_puesto_nom,2,1)||'L'||TC_ATRIBUTO_PUESTO.id_nivel_pto||substr(TN_MOVTOS_EMP_PLA.id_puesto_nom,3,5)
                    when TN_MOVTOS_EMP_PLA.id_unidad_nom = 'G00' then substr(TN_MOVTOS_EMP_PLA.id_puesto_nom,1,2)||TC_ATRIBUTO_PUESTO.id_nivel_pto||substr(TN_MOVTOS_EMP_PLA.id_puesto_nom,3,5)        --G00
                    else TN_MOVTOS_EMP_PLA.id_puesto_nom
               end
         WHEN TN_MOVTOS_EMP_PLA.MEP_QNA_CAPTURA < 201618 and tn_movtos_emp_pla.id_unidad_nom = 'G00' THEN
              substr(TN_MOVTOS_EMP_PLA.id_puesto_nom,1,4)||substr(TN_MOVTOS_EMP_PLA.id_puesto_nom,7,1) ||'00'||substr(TN_MOVTOS_EMP_PLA.id_puesto_nom,5,3)
         WHEN TN_MOVTOS_EMP_PLA.MEP_QNA_CAPTURA >= 201618 and tn_movtos_emp_pla.id_unidad_nom = 'G00' THEN
              substr(TN_MOVTOS_EMP_PLA.id_puesto_nom,1,2)||TC_ATRIBUTO_PUESTO.id_nivel_pto||substr(TN_MOVTOS_EMP_PLA.id_puesto_nom,3,5)        --G00
         ELSE TN_MOVTOS_EMP_PLA.id_puesto_nom END ID_PUESTO_NOM,
    TC_ATRIBUTO_PUESTO.ID_NIVEL_PTO,
    CASE WHEN  TN_MOVTOS_EMP_PLA.ID_EDO_NOM IS NULL THEN '00' WHEN TRIM( TN_MOVTOS_EMP_PLA.ID_EDO_NOM) = '' THEN '00' ELSE lpad(TN_MOVTOS_EMP_PLA.ID_EDO_NOM,2,'0') END ESTADO_PLAZA,
    CASE WHEN  TN_MOVTOS_EMP_PLA.ID_MUNI_NOM IS NULL THEN '000' WHEN TRIM( TN_MOVTOS_EMP_PLA.ID_MUNI_NOM) = '' THEN '000' ELSE TO_CHAR(TN_MOVTOS_EMP_PLA.ID_MUNI_NOM)
    END MUNICIPIO_PLAZA,
    TN_MOVTOS_EMP_PLA.ID_ZONA_ECO_NOM, nvl(td_plaza.ID_ZONA_DIST_NOM,'9000') ID_ZONA_DIST_NOM,
    CASE WHEN TN_MOVTOS_EMP_PLA.ID_GRUPO_PAGO IN ('E','J') AND TC_ATRIBUTO_PUESTO.ID_JERARQUIA < '7'  THEN TD_ESCENARIO.NOMBRE_PUESTO
            WHEN TN_MOVTOS_EMP_PLA.ID_GRUPO_PAGO = 'V' AND SUBSTR(TD_EMPLEADO.CURP_EMPLEADO,11,  1) = 'H'  THEN TRIM(TC_PUESTO.DESC_PUESTOHO)
            WHEN TN_MOVTOS_EMP_PLA.ID_GRUPO_PAGO = 'V' AND SUBSTR(TD_EMPLEADO.CURP_EMPLEADO,11,  1) = 'M'  THEN TRIM(TC_PUESTO.DESC_PUESTOMU)
            WHEN TC_ATRIBUTO_PUESTO.ID_JERARQUIA >= '7' AND SUBSTR(TD_EMPLEADO.CURP_EMPLEADO,11,  1) = 'H'  THEN TRIM(TC_PUESTO.DESC_PUESTOHO)
            WHEN TC_ATRIBUTO_PUESTO.ID_JERARQUIA >= '7' AND SUBSTR(TD_EMPLEADO.CURP_EMPLEADO,11,  1) = 'M'  THEN TRIM(TC_PUESTO.DESC_PUESTOMU)
            ELSE TRIM(TC_PUESTO.DESC_PUESTO)  END DENOMINACION_FUNCIONAL,
    TC_TABULADOR.TAB_SUELDO,
    TC_TABULADOR.TAB_COMPENSACION,
    TC_TABULADOR.TAB_SUELDO +  TC_TABULADOR.TAB_COMPENSACION,
    CASE WHEN SUBSTR(TD_EMPLEADO.CURP_EMPLEADO, 11,1) = 'M' THEN 'TRABAJADORA' WHEN SUBSTR(TD_EMPLEADO.CURP_EMPLEADO, 11,1) = 'H' THEN 'TRABAJADOR' ELSE 'TRABAJADOR' END TITULO_FIRMA,
    to_char(TN_MOVTOS_EMP_PLA.MEP_INI_CONT, 'dd') || ' ' ||
    trim(to_char(TN_MOVTOS_EMP_PLA.MEP_INI_CONT, 'MONTH')) || ' ' ||
    to_char(TN_MOVTOS_EMP_PLA.MEP_INI_CONT, 'yyyy') INICIO,
    CASE
    WHEN TN_MOVTOS_EMP_PLA.MEP_FIN_CONT >= TO_DATE('01/01/2099','DD/MM/YYYY')  THEN 'INDEFINIDO'
    ELSE     to_char(TN_MOVTOS_EMP_PLA.MEP_FIN_CONT, 'dd') || ' ' ||
    trim(to_char(TN_MOVTOS_EMP_PLA.MEP_FIN_CONT, 'MONTH')) || ' ' ||
    to_char(TN_MOVTOS_EMP_PLA.MEP_FIN_CONT, 'yyyy') END FINAL,
    TRIM(TRIM(TD_EMPLEADO.PRIMER_APELLIDO)||' '||TRIM(TD_EMPLEADO.SEGUNDO_APELLIDO)||' '||TRIM(TD_EMPLEADO.NOMBRE_EMPLEADO)) NOMBRE_COMPLETO,
   CASE WHEN TC_ATRIBUTO_PUESTO.ID_JERARQUIA >= '7' THEN 'Conforme al Último Catálogo de Puestos Autorizado.' ELSE 'Conforme al Último Catálogo de Puestos Autorizado, y/o los que se deriven de la Normatividad vigente aplicable.' END FUNCION_ESPECIFICA,
    TRIM(NVL(TC_REFERENCIA.REF_ELABORO_TITULO,'-')) ELABORO_TITULO,
    TRIM(TC_REFERENCIA.REF_ELABORO) ELABORO,
    TC_REFERENCIA.REF_FRACC_VOBO REFERENCIA_VOBO,
    TRIM(TC_REFERENCIA.REF_VOBO_TITULO) VOBO_TITULO,
    TRIM(TC_REFERENCIA.REF_VOBO) VOBO,
    TC_REFERENCIA.REF_FRACC_AUTORIZO TITULO_REVISO,
    TRIM(TC_REFERENCIA.REF_AUTORIZO_TITULO) AUTORIZO_TITULO,
    TRIM(TC_REFERENCIA.REF_AUTORIZO) AUTORIZO,
    'Protesto, conforme a lo dispuesto por el Artículo 128 de la Constitución Política de los Estados Unidos Mexicanos, guardar ésta y las Leyes que de ella emanen. Así mismo manifiesto que los datos anteriormente señalados son ciertos y me hago sabedor sobre las penas que se le impondrán a quien faltare a la verdad como lo disponen los Artículos 245  y 247 del Código Penal Federal.' PROTESTA,
    NVL(TN_MOVTOS_EMP_PLA.MEP_OBSERVA,' ') OBSERVACIONES,
    CASE WHEN TN_MOVTOS_EMP_PLA.ID_EDO_NOM = 9 THEN 'MPO' ELSE 'MPO' END TITULO_MUN,
       PKG_DATOS_DEPENDENCIA.FUNC_UNIDAD_OM() AS UNI_OM,
       PKG_DATOS_DEPENDENCIA.FUNC_UNIDAD_RH() AS UNI_RH
FROM
    TN_MOVTOS_EMP_PLA,
    TD_EMPLEADO,
    TC_MOVS_PERSONAL,
    TC_GENERO,
    TC_ATRIBUTO_PUESTO,
    TC_UNIDAD,
    TC_PUESTO,
    TC_TABULADOR,
    TC_REFERENCIA,
    TC_NACIONALIDAD,
    TD_ESCENARIO,
    TC_ESTADO EDO,
    TC_MUNICIPIO,
    TC_ESTADO,
    TD_PLAZA
WHERE
    TN_MOVTOS_EMP_PLA.ID_MOVTOS_EMP_PLA = $P{PAR_ID_MOVTO}
AND TN_MOVTOS_EMP_PLA.MEP_RFC = TD_EMPLEADO.RFC_EMPLEADO
AND TN_MOVTOS_EMP_PLA.ID_UNIDAD_NOM = TC_UNIDAD.ID_UNIDAD(+)
AND TC_UNIDAD.UNI_CICLO IN (SELECT SUBSTR(RC_QNA_CAPTURA,0,4) FROM TN_REG_CONTROL WHERE TN_REG_CONTROL.RC_VIGENTE   = 'S')
AND TN_MOVTOS_EMP_PLA.ID_PUESTO_NOM = TC_PUESTO.ID_PUESTO
AND TN_MOVTOS_EMP_PLA.ID_UNIDAD_NOM = TC_REFERENCIA.ID_UNIDAD_FISICA
AND TN_MOVTOS_EMP_PLA.MEP_INI_CONT BETWEEN TO_CHAR(TC_REFERENCIA.REF_FECHA_DOS, 'dd/mm/yyyy') AND TO_CHAR(TC_REFERENCIA.REF_FECHA_TRES, 'dd/mm/yyyy')
AND TC_REFERENCIA.REF_CIFRAS1 = 100
AND TRIM(TC_REFERENCIA.REF_PROGRAMA) = 'NOMBRAMIENTO'
AND TD_EMPLEADO.ID_EDO_PARTICULAR = TC_MUNICIPIO.ID_EDO(+)
AND TD_EMPLEADO.ID_MUNI_PARTICULAR = TC_MUNICIPIO.ID_MUNICIPIO(+)
AND TD_EMPLEADO.ID_EDO_PARTICULAR = TC_ESTADO.ID_EDO(+)
AND TN_MOVTOS_EMP_PLA.ID_MOVTO_PERSONA = TC_MOVS_PERSONAL.ID_MOVS_PERSONAL
AND TN_MOVTOS_EMP_PLA.ID_PUESTO_NOM = TC_ATRIBUTO_PUESTO.ID_ATRIBUTO_PUESTO
AND TN_MOVTOS_EMP_PLA.MEP_INI_CONT BETWEEN TO_CHAR(TC_ATRIBUTO_PUESTO.AP_INI, 'dd/mm/yyyy') AND TO_CHAR(TC_ATRIBUTO_PUESTO.AP_FIN, 'dd/mm/yyyy')
AND TC_ATRIBUTO_PUESTO.ID_NIVEL_PTO = TC_TABULADOR.ID_NIVEL_PTO
AND TN_MOVTOS_EMP_PLA.ID_ZONA_ECO_NOM = TC_TABULADOR.ID_ZONA_ECON
AND TN_MOVTOS_EMP_PLA.ID_RANGO_NOM = TC_TABULADOR.ID_RANGO
AND TN_MOVTOS_EMP_PLA.MEP_INI_CONT BETWEEN TO_CHAR(TC_TABULADOR.TAB_INI, 'dd/mm/yyyy') AND TO_CHAR(TC_TABULADOR.TAB_FIN, 'dd/mm/yyyy')
AND TN_MOVTOS_EMP_PLA.ID_PLAZA = TD_ESCENARIO.PLAZA_ESCENARIO(+)
AND TD_ESCENARIO.FIN_ESCENARIO(+) = TO_DATE('31/12/2099','DD/MM/YYYY')
AND TN_MOVTOS_EMP_PLA.ID_EDO_NOM = EDO.ID_EDO(+)
AND TN_MOVTOS_EMP_PLA.ID_PLAZA = TD_PLAZA.ID_PLAZA(+)
AND TD_EMPLEADO.ID_GENERO = TC_GENERO.ID_GENERO
AND TD_EMPLEADO.ID_NACIONALIDAD = TC_NACIONALIDAD.ID_NACIONALIDAD(+)
AND ROWNUM < 2]]>
	</queryString>
