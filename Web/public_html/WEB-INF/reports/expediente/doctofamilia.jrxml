		<![CDATA[SELECT PZA.ID_UNIDAD_NOM UNIDAD, EF.RFC_EMPLEADO RFC_EMPLEADO, NVL(EMP.PRIMER_APELLIDO,' ')||' '||NVL(EMP.SEGUNDO_APELLIDO,' ')||' '||EMP.NOMBRE_EMPLEADO NOMBRE_EMPLEADO, EF.FDB_NOMBRE_EMPLEADO ||' '|| EF.FDB_PRIMER_APELLIDO ||' '||EF.FDB_SEGUNDO_APELLIDO NOMBRE_FAMILIAR, EF.FDB_FECHA_NACIMIENTO FECHA_NACIMIENTO, UPPER(GE.DESC_GENERO) GENERO, UPPER(PA.DESC_PARENTESCO) AS PARENTESCO, UPPER(TN.DESC_NIVEL_ESCOLAR) AS NIVEL_ESCOLAR, EF.FDB_PROMEDIO PROMEDIO, RE.DESC_RECHAZO RECHAZO, ST.DES_STATUS STATUS,
PKG_DATOS_DEPENDENCIA.FUNC_DOMICILIO() AS DOMICILIO,
PKG_DATOS_DEPENDENCIA.FUNC_UNIDAD_OM() AS UNI_OM,
PKG_DATOS_DEPENDENCIA.FUNC_UNIDAD_RH() AS UNI_RH
FROM TD_EXP_FAMILIAR_DEPS_BENEF EF, TC_TIPO_DOCTOS_EXPEDIENTE DOC, TC_ESTADO_REGISTRO ST, TC_EXP_RECHAZO RE, TC_PARENTESCO PA, TC_NIVEL_ESCOLAR TN, TC_GENERO GE, TD_EMPLEADO EMP, TD_PLAZA PZA
WHERE EF.ID_DOCTO         = DOC.ID_DOCTO
  AND EF.ID_STATUS        = ST.ID_STATUS
  AND EF.ID_EXP_RECHAZO   = RE.ID_EXP_RECHAZO(+)
  AND EF.ID_PARENTESCO    = PA.ID_PARENTESCO(+)
  AND EF.ID_NIVEL_ESCOLAR = TN.ID_NIVEL_ESCOLAR(+)
  AND EF.ID_GENERO        = GE.ID_GENERO(+)
  AND ((EF.RFC_EMPLEADO = $P{RFC} AND $P{UNI} IS NULL) OR
       ($P{RFC} IS NULL AND PZA.ID_UNIDAD_NOM = $P{UNI}) OR
	   (EF.RFC_EMPLEADO = $P{RFC} AND PZA.ID_UNIDAD_NOM = $P{UNI}))
  AND EF.RFC_EMPLEADO     = EMP.RFC_EMPLEADO
  AND EF.RFC_EMPLEADO     = PZA.PLAZA_RFC
ORDER BY EF.RFC_EMPLEADO]]>
	</queryString>
