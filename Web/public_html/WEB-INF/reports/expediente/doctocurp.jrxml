		<![CDATA[SELECT PZA.ID_UNIDAD_NOM UNIDAD, EC.RFC_EMPLEADO, EMP.PRIMER_APELLIDO||' ' ||EMP.SEGUNDO_APELLIDO||' ' ||EMP.NOMBRE_EMPLEADO NOMBRE, EC.CURP_CVE CURP, EC.CURP_FOLIO, RE.DESC_RECHAZO AS RECHAZO, ST.DES_STATUS STATUS,
PKG_DATOS_DEPENDENCIA.FUNC_DOMICILIO() AS DOMICILIO,
       PKG_DATOS_DEPENDENCIA.FUNC_UNIDAD_OM() AS UNI_OM,
       PKG_DATOS_DEPENDENCIA.FUNC_UNIDAD_RH() AS UNI_RH
FROM TD_EXP_CURP EC, TC_TIPO_DOCTOS_EXPEDIENTE DOC, TC_ESTADO_REGISTRO ST, TC_EXP_RECHAZO RE, TD_EMPLEADO EMP, TD_PLAZA PZA
WHERE EC.ID_DOCTO       = DOC.ID_DOCTO
  AND EC.ID_STATUS      = ST.ID_STATUS
  AND EC.ID_EXP_RECHAZO = RE.ID_EXP_RECHAZO(+)
  AND ((EC.RFC_EMPLEADO   = $P{RFC} AND $P{UNI} IS NULL) OR
       ($P{RFC} IS NULL AND PZA.ID_UNIDAD_NOM = $P{UNI}) OR
	   (EC.RFC_EMPLEADO   = $P{RFC} AND PZA.ID_UNIDAD_NOM = $P{UNI}))
  AND EC.RFC_EMPLEADO   = EMP.RFC_EMPLEADO
  AND EC.RFC_EMPLEADO   = PZA.PLAZA_RFC
ORDER BY EC.RFC_EMPLEADO]]>
