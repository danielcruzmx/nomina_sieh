				desc="acepta caracteres"
				class="com.blackbear.flatworm.converters.CoreConverters"
				method="convertChar"
				return-type="java.lang.String"/>
		<tipo 	name="decimal"
				desc="acepta numeros"
				class="com.blackbear.flatworm.converters.CoreConverters"
				method="convertDecimal"
				return-type="java.lang.Double"/>
		<tipo 	name="monto"			
				desc="acepta numeros y formatea el monto con punto decimal"
